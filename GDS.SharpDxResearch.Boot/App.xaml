<Application x:Class="GDS.SharpDxResearch.Boot.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GDS.SharpDxResearch.Boot"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="pack://application:,,,/GDS.SharpDxResearch.GraphicObjects;component/Resources.xaml"/>
                    
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        <DataTemplate DataType="{x:Type vm:DxObjectViewModel}">
            <v:DxObject/>
        </DataTemplate>
    </Application.Resources>
</Application>
